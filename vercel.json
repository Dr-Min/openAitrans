{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "functions": {
    "app.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "SECRET_KEY": "@secret_key",
    "ANTHROPIC_API_KEY": "@anthropic_api_key",
    "JWT_SECRET_KEY": "@jwt_secret_key"
  }
}
